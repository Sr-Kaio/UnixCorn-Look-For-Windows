{
    "workbench.startupEditor": "newUntitledFile",
    "svelte.enable-ts-plugin": true,
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "cmake.buildArgs": [
        "-G \"MinGW Makefiles\""
    ],
    "explorer.autoReveal": true,
    "workbench.iconTheme": "catppuccin-perfect-macchiato",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.lineHeight": 1.8,
    "editor.renderLineHighlight": "gutter",
    "explorer.compactFolders": false,
    "breadcrumbs.enabled": false,
    "editor.minimap.autohide": true,
    "workbench.sideBar.location": "right",
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.colorTheme": "Catppuccin Mocha",
    "animations.Install-Method": "Custom UI Style",
    "apc.imports": [
        "file:///c:/Users/caiop/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
    ],
    "vscode_custom_css.imports": [
        "file:///c:/Users/caiop/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
    ],
    "custom-ui-style.external.imports": [
        "file:///c:/Users/caiop/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
    ],
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "custom-ui-style.stylesheet": {
        ".windows" : {
            "font-family": "JetBrains Mono",
        },
        ".windows *" : {
            "font-family": "JetBrains Mono",
        },
        ".windows div *" : {
            "border-radius": "0px",
        },

        ".titlebar" : {
            "padding-left": "10px"
        },
        ".part .sidebar" : {
            "drop-shadow" : "0px 0px 50px rgba(0, 0, 0, .25)"
        },
        ".slider" : {
            "position": "absolute !important",
            "right": "1px !important",
            "width": "1px !important",
            "left": "auto !important"
        },
        ".monaco-editor-hover,.monaco-hover" : {
            "box-shadow": "0px 8px 32px rgba(0,0,0,.45) !important",
            
            "backdrop-filter": "blur(3px) !important",
            "border": "none !important",
            "outline": "0px !important"
        },
        ".quick-input-widget" : {
            "transform": "translateY(-50%) !important",
            "top": "50% !important",
            "box-shadow": "0px 8px 32px rgba(0, 0, 0, .35) !important",
            "padding": "10px 10px 18px 10px !important",
            "backdrop-filter": "blur(3px) !important"
        },
        
        ".quick-input-list, .quick-input-list .list_id_6" : {
            "min-height": "400px !important"
        },
        ".window-appicon" : {
            "display": "none !important",
        },
        ".titlebar-center" : {
            "display": "none !important",
        },
            ".monaco-workbench .activitybar>.content" : {
            "justify-content": "center"
          },
          
          ".monaco-workbench .activitybar>.content>.composite-bar" : {
            "margin-bottom": "inherit"
          },
          
          
          ".monaco-workbench .activitybar>.content :not(.monaco-menu)>.monaco-action-bar .action-item.checked .active-item-indicator:before" : {
            "border-left-color": "transparent !important"
          },
          
          ".monaco-workbench .activitybar>.content :not(.monaco-menu)>.monaco-action-bar .action-item" : {
            "margin-bottom": "4px",
            "width": "36px",
            "height": "36px",
            "margin-left": "4px",
          },
          
          "div#workbench.parts.activitybar" : {
            "background-color": "var(--vscode-editorWidget-background) !important"
          },
          
          "li.action-item.icon" : {
            "border": "2px solid transparent"
          },
          
          "li.action-item.icon:hover" : {
            "border": "2px solid var(--vscode-icon-foreground)"
          },
          
          "li.action-item.icon.checked:hover .codicon:before" : {
            "color": "var(--vscode-activityBar-activeBorder) !important"
          },
          
          "li.action-item.icon.checked" : {
            "border": "2px solid var(--vscode-activityBar-activeBorder)"
          },
          
     
          
          ".monaco-workbench .activitybar>.content :not(.monaco-menu)>.monaco-action-bar .action-label.codicon,.monaco-workbench .activitybar>.content :not(.monaco-menu)>.monaco-action-bar .action-label:not(.codicon)" : {
            "width": "36px !important",
            "height": "36px !important"
          },
          
          ".monaco-workbench .activitybar>.content :not(.monaco-menu)>.monaco-action-bar .action-label:not(.codicon)" : {
            "padding": 0
          },
          
         
          
          ".monaco-workbench .activitybar>.content :not(.monaco-menu)>.monaco-action-bar .action-item:focus .active-item-indicator:before" : {
            "border-left-color": "transparent !important"
          },
          ".monaco-workbench .activitybar>.content :not(.monaco-menu)>.monaco-action-bar .badge .badge-content" : {
            "position": "absolute",
            "top": "16px",
            "right": "4px"
          },
          
    },
    "chat.commandCenter.enabled": false,
    "workbench.navigationControl.enabled": false,
    "workbench.layoutControl.enabled": false,
    "workbench.statusBar.visible": false,
    "window.commandCenter": false,
    "editor.minimap.enabled": false
}